{
    "name": "quinen/cake",
    "description": "QuinenCake plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "quinen/php-lib": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "QuinenCake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuinenCake\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
